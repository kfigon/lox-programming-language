function fibo(i) {
    if (i <= 1) {
        print(i);
    } else {
        prevPrev = 0;
        prev = 0;
        curr = 1;

        cntr = 1;
        while (cntr < i) {
            prevPrev = prev;
            prev = curr;
            curr = prevPrev + prev;
            
            cntr = cntr + 1;
        }
        print(curr);
    }
}


fibo(8);